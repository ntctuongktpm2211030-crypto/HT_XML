<!ELEMENT elearning (course:course+)>
<!ELEMENT course:course (course:title, instructor:instructor, course:students)>
<!ATTLIST course:course id CDATA #REQUIRED>
<!ATTLIST course:course category CDATA #REQUIRED>
<!ELEMENT course:title (#PCDATA)>
<!ELEMENT instructor:instructor (instructor:name, instructor:email)>
<!ATTLIST instructor:instructor id CDATA #REQUIRED>
<!ELEMENT instructor:name (#PCDATA)>
<!ELEMENT instructor:email (#PCDATA)>
<!ELEMENT course:students (student:student+)>
<!ELEMENT student:student (student:name)>
<!ATTLIST student:student id CDATA #REQUIRED>
<!ATTLIST student:student progress CDATA #REQUIRED>
<!ELEMENT student:name (#PCDATA)>
